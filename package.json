{
  "version": "1.0.0",
  "description": "Adds in effects to an image",
  "main": "main.js",
  "scripts": {
    "build": "npm run webpack && npm run assets",
    "bundle": "npm run build && rm -rf out && electron-packager . image-effects --out=out --ignore=\"(node_modules|src|assets)\"",
    "clean": "rm -rf dist",
    "dev": "webpack --watch -d",
    "electron": "electron .",
    "assets": "cp assets/dat.gui.min.js dist/dat.gui.min.js",
    "webpack": "webpack"
  },
  "keywords": [
    "image",
    "effects"
  ],
  "author": "Scott McKenzie",
  "license": "ISC",
  "dependencies": {
    "fluent-ffmpeg": "^2.1.0",
    "image-size": "^0.5.1",
    "postprocessing": "^2.1.0",
    "three": "^0.84.0"
  },
  "devDependencies": {
    "css-loader": "^0.27.3",
    "electron": "^1.6.2",
    "electron-packager": "^8.6.0",
    "node-sass": "^4.5.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  }
}
